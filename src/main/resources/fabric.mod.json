{
  "schemaVersion": 1,
  "id": "minemenufabric",
  "version": "${version}",
  "name": "MineMenuFabric",
  "description": "MineMenu for fabric",
  "authors": [
    "UltraBlackLinux"
  ],
  "contact": {
    "website": "https://github.com/UltraBlackLinux/MineMenuFabric",
    "repo": "https://github.com/UltraBlackLinux/MineMenuFabric/issues"
  },
  "license": "MIT",
  "icon": "assets/minemenufabric/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.ultrablacklinux.minemenufabric.client.MineMenuFabricClient"
    ],
    "modmenu": [
      "me.ultrablacklinux.minemenufabric.client.config.modmenu.ModMenuEntry"
    ]
  },
  "mixins": [
    "MineMenuFabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "*",
    "modmenu": ">=1.16.7",
    "cloth-config2": ">=4.10.6"
  },
  "custom": {
    "modmenu": [
      "me.ultrablacklinux.minemenufabric.config.config."
    ]
  }
}
